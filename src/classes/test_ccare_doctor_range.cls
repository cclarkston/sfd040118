  static testMethod void test_navigation_methods() {
    init();
    test.startTest();
    Test.setMock(HttpCalloutMock.class, new http_mock_dosespot());
    
    PageReference pageref = new PageReference('/apex/ccare_clinical_doctor_range_review');
    Test.setCurrentPage(pageRef);    
    dp_clinical_doc_review_range doc_rev_controller = new dp_clinical_doc_review_range();
    
    try {doc_rev_controller.change_center();} catch (Exception e) { //should fail - no cid
    }
    
    //cycle through all centers
    for(Center_Information__c ci : doc_rev_controller.getcenter_list()) {
      pageRef.getParameters().put('cid',ci.id);  
      doc_rev_controller.change_center();
    }
    
    doc_rev_controller.getcenter_list();      
    
    System.assertEquals(doc_rev_controller.getgear_menu().size(),1);
    
    try {
      pageRef.getParameters().put('navid','BAD ID'); 
      doc_rev_controller.navigate_to();
    } catch(Exception e) {
      //should fail - bad navid
    }    
    
    //good init with starting patient id and center    
    Account a = [select id,center__c from Account limit 1];
    pageRef.getParameters().put('ctrid',a.center__c);  
    pageRef.getParameters().put('sdate',System.today().adddays(-15).format());
    pageRef.getParameters().put('edate',System.today().format());
    doc_rev_controller = new dp_clinical_doc_review_range();    
    
    //cycle through nav items
    for(dp_clinical_doc_review_range.menu_item mi : doc_rev_controller.getnavigation_menu()) {
      //tests for object methods if necessary
      if(mi.menu_type=='Page') {
        pageRef.getParameters().put('navid',mi.menu_id);    	
        PageReference newpage = doc_rev_controller.navigate_to();
      }
    }    
    
    System.assertEquals(doc_rev_controller.getmain_menu().size(),2);
    System.assertEquals(doc_rev_controller.getclinical_header_items().size(),1);
    
    //check error count
    doc_rev_controller.getdosespot_notification_count(); 
    
    List<RecentlyViewed> rview = doc_rev_controller.getrecent_records();
    pageRef.getParameters().put('recentid',rview[0].id);
    doc_rev_controller.navigate_to_recent_patient();   
    
    //fail default center
    try {
      pageRef.getParameters().put('ctrid','BAD ID'); 
      doc_rev_controller = new dp_clinical_doc_review_range();
    } catch (Exception e) {}
       
    test.stopTest();
  }   
  
