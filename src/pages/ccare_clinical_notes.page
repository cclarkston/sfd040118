                    <apex:outputpanel styleclass="cc-perio-prosth-row cc-form-checkbutton-container" style="height:100%;" rendered="{!custom.currently_selected || custom.custom_note.IsActive__c}">
                      <div class="cc-perio-prosth-label ">
                        <div class="cc-form-checkbutton-{!if(custom.currently_selected,'on','off')}" onclick="toggle_cnote_selection('{!sform.cnote.id}','{!custom.custom_note.id}');toggle_radio_button(this);">
                        </div>
                      </div>
                      <div class="cc-form-checkbutton-label">{!custom.custom_note.Note_Text__c} </div>
                      
                    </apex:outputpanel>             
                    <apex:outputpanel layout="block" styleclass="cc-patientProfile-input-container" rendered="{!custom.currently_selected}" style="width:100%;padding-left:100px;padding-right:20px;">
                      <apex:inputtextArea rows="3" value="{!custom.note_record.Comments__c}" onchange="changes_made();" onkeydown="if(event.keyCode==13){this.blur();}" onkeyup="note_count_check('tcount_{!custom.custom_note.id}',this.value);" style="width:85%;" />
                      <span id="tcount_{!custom.custom_note.id}" style="display: inline-flex;width: 10%;padding-left:10px;">
                        {!250 - LEN(custom.note_record.Comments__c)}
                      </span>
                    </apex:outputpanel>         
                  </apex:outputpanel>
                  <!-- if form can not be edited -->
                  <apex:outputpanel layout="block" style="width:100%" rendered="{!if(sform.isform_editable,false,true)}">
                    <apex:outputpanel layout="block" styleclass="cc-perio-prosth-row cc-form-checkbutton-container" style="height:100%;" rendered="{!custom.currently_selected}">
                      <div class="cc-perio-prosth-label " style="width:inherit;">
                        <div class="cc-form-checkbutton-{!if(custom.currently_selected,'on','off')}" style="display:none;">
                        </div>
                      </div>
                      <div class="cc-form-checkbutton-label">{!custom.custom_note.Note_Text__c} 
                        <apex:outputpanel layout="block" styleclass="cc-patientProfile-input-container" rendered="{!custom.currently_selected}">
                          <apex:outputfield value="{!custom.note_record.Comments__c}" styleclass="cc-patientProfile-input" />
                          <div class="cc-inputBottomBorder"></div>
                          <div class="cc-inputBottomBorder-off"> </div>
                        </apex:outputpanel>
                      </div>
                    </apex:outputpanel>      
                  </apex:outputpanel>                                                              
                </apex:repeat>
                
                <div class="cc-perio-prosth-row" style="">
                  <div class="cc-form-input-label-markSelected cc-perio-prosth-label">Additional<br />Notes:</div>
                  <div class="cc-patientProfile-input-container" style="width:100%;">
                    <div class="cc-form-textAreaContainer cc-form-input-markSelected">
                      <apex:inputfield rendered="{!sform.isform_editable}" value="{!sform.cnote.Apt_Note__c}" onchange="changes_made();" style="resize: none;{!if(sform.isform_editable==false,'cursor:not-allowed; ','')}"/>
                      <apex:outputfield rendered="{!if(sform.isform_editable,false,true)}" value="{!sform.cnote.Apt_Note__c}" style="{!if(sform.isform_editable==false,'cursor:not-allowed; ','')}" />
                    </div>
                  </div>
                </div>
                <!-- start row -->
			    <div class="cc-perio-prosth-row">
			      <div class="cc-perio-prosth-label cc-form-input-label">&nbsp;</div>
			      <div class="cc-patientProfile-input-container">	
  			        <apex:outputpanel layout="block" styleclass="cc-form-notation-button" rendered="{!if(sform.allow_review && is_user_doctor,true,false)}" onclick="review_note('{!sform.cnote.id}');">Doctor Reviewed</apex:outputpanel>
  			        <apex:outputpanel layout="block" rendered="{!if(sform.allow_review,false,true)}" style="font-style:italic;">
  			          <apex:outputtext value="{!sform.review_line}" escape="false" />
  			        </apex:outputpanel>
			      </div>
			    </div>
			    <!-- end row -->
                
              </div>
              </apex:outputpanel>              
            </div>
          </apex:outputpanel>
            <apex:variable var="form_count" value="{!form_count + 1}"/>
            </apex:repeat>
            <!-- end loop through all standard forms -->    

          <!-- </div>
        </div> -->
      </apex:repeat>
      <!-- end loop through all apts that should have notes -->
    </apex:outputpanel>
  
  
  </apex:outputpanel>
  
  
  <!-- START PATIENT SEARCH MODAL------------------>
  <apex:outputpanel layout="block" rendered="{!display_patient_search_modal}">
  <div class="cc-patient-patient-search-wrapper" style="z-index: 10100; opacity: 1;">
    <div class="cc-patient-patient-search-container" style="box-shadow: rgba(0, 0, 0, 0.74902) 0px 0px 70px -15px;">
      <div class="cc-patient-patient-search-close">
        <div id="cc-patient-patient-search-close-button" onclick="cpatient_search();">
          <i class="ccf-cancel-thin cc-color-font-black cc-cursor-hover"></i>
        </div>
      </div>
      <div style="padding-bottom:10px;">
      <div class="cc-patient-patient-search-info" style="display: flex;justify-content: space-between; flex-wrap: wrap;">
        <div class="cc-patientProfile-row" style="width:50%;">
          <div class="cc-patientProfile-label">Last Name:</div>
          <div class="cc-patientProfile-input-container">
            <apex:input styleclass="cc-patientProfile-input psearch_name psearch" value="{!search_name}" onkeydown="if(event.keyCode==13){this.blur();}" onkeyup="qsearch_key(event,this.value.length);" />
            <div class="cc-inputBottomBorder"> </div>
            <div class="cc-inputBottomBorder-off"> </div>
          </div>
        </div>      
        <div class="cc-patientProfile-row" style="width:50%;">
          <div class="cc-patientProfile-label">First Name:</div>
          <div class="cc-patientProfile-input-container">
            <apex:input styleclass="cc-patientProfile-input psearch" value="{!search_first}" onkeydown="if(event.keyCode==13){this.blur();}" onkeyup="qsearch_key(event,this.value.length);" />
            <div class="cc-inputBottomBorder"> </div>
            <div class="cc-inputBottomBorder-off"> </div>
          </div>
        </div>  
        <div class="cc-patientProfile-row" style="width:50%;">
          <div class="cc-patientProfile-label">Phone:</div>
          <div class="cc-patientProfile-input-container">
            <apex:input styleclass="cc-patientProfile-input psearch" value="{!search_phone}" onkeydown="if(event.keyCode==13){this.blur();}" onkeyup="qsearch_key(event,this.value.length);"/>
            <div class="cc-inputBottomBorder"> </div>
            <div class="cc-inputBottomBorder-off"> </div>
          </div>
        </div>
        <div class="cc-patientProfile-row" style="width:50%;">
          <div class="cc-patientProfile-label">Email:</div>
          <div class="cc-patientProfile-input-container">
            <apex:input styleclass="cc-patientProfile-input psearch" value="{!search_email}" onkeydown="if(event.keyCode==13){this.blur();}" onkeyup="qsearch_key(event,this.value.length);" />
            <div class="cc-inputBottomBorder"> </div>
            <div class="cc-inputBottomBorder-off"> </div>
          </div>
        </div>
        <div class="cc-patientProfile-row" style="width:50%;margin-top:18px;align-self: flex-start;">
          <div class="cc-patientProfile-label">Unique ID:</div>
          <div class="cc-patientProfile-input-container">
            <apex:input styleclass="cc-patientProfile-input psearch" value="{!search_unique}" onkeydown="if(event.keyCode==13){this.blur();}" onkeyup="qsearch_key(event,this.value.length);" />
            <div class="cc-inputBottomBorder"> </div>
            <div class="cc-inputBottomBorder-off"> </div>
          </div>
        </div>        
        <apex:outputpanel rendered="{!if(allow_quick_search,false,true)}" layout="block" styleclass="cc-patientProfile-row" style="width:50%;align-items:flex-end;">
          <div class="cc-form-confirmTrash-button cc-form-confirmReschedule-button-confirm" onclick="direct_search_patients();">Search</div>
          <!-- <span class="spacer">&nbsp;</span>
          <apex:commandLink status="ajaxstatus" action="{!toggle_quick_search}" reRender="function_holder,main_holder" value="Enable Quick Search" styleclass="lblue" style="align-self:center;"/> -->            
        </apex:outputpanel>
      </div>         
      </div>        
      <apex:outputpanel layout="block" styleclass="cc-patient-schedule-search-results" rendered="{!if(search_result_size>0,true,false)}">
        <div class="cc-patient-schedule-search-results-row-container" id="cc-patient-patient-search-results-row-container" style="opacity: 1;">
          <div class="cc-patient-schedule-search-results-row-label">Search Results</div>
          <div class="cc-patient-patient-search-results-row-options">
            <apex:repeat var="patient" value="{!consult_search_results}" >
              <div class="cc-patient-patient-search-results-row-result" style="margin-top:10px;margin-bottom:20px;" onclick="view_patient('{!patient.id}');">
                <div class="cc-patient-patient-search-results-row-result-name">{!patient.name}</div>
                <div>{!patient.phone}</div>
                <div>{!patient.billingstreet}</div>
                <div>{!if(patient.Email__c=='',patient.personemail,patient.Email__c)}</div>
                <div>{!patient.Warranty_ID__c}</div>
              </div>
            </apex:repeat>
          </div>
          <apex:outputpanel layout="block" rendered="{!display_more_search_records}" style="display:flex;">
            <div class="cc-form-notation-button" onclick="more_patients();">More Matches</div>
          </apex:outputpanel>
        </div>
      </apex:outputpanel><!--end search results-->      
    </div>
  </div>
  </apex:outputpanel><!---END PATIENT SEARCH MODAL----->
  
  <!---START Variable Tooth Selection Modal------->
  <apex:outputpanel layout="block" rendered="{!display_var_tooth_modal}">  
  <apex:outputpanel layout="block"  styleclass="cc-clinical-addPOT-wrapper" style="z-index: 1100; opacity: 1;" id="var_tooth_holder">
    <div class="cc-clinical-addPOT-container" style="box-shadow: rgba(0, 0, 0, 0.74902) 0px 0px 70px -15px;">
    <apex:messages style="display:inline;" />
      <div class="cc-clinical-addPOT-close">
        <div onclick="cvar_tooth_modal();">
          <i class="ccf-cancel-thin cc-color-font-black cc-cursor-hover"></i>
        </div>
      </div>      
      
      <!--START POT Selection Holder-->
      <div style="width:100%;">
        <div class="cc-clinical-addPOT-section-topHeader">{!tooth_chart_label} - {!current_variable_tooth_selections}</div>
               
        <!--POT Search Holder---->  
        <div style="width:100%;">
          <div class="cc-perio-teeth-row-container-upper">
            <apex:repeat var="tooth" value="{!upper_variable_tooth_chart}">
              <div class="cc-perio-teeth-cell-container {!if(tooth.tooth_selected,'cc-perio-selected','')}" onclick="toggle_var_tooth('{!tooth.tooth_num}');" >
                <div class="cc-perio-teeth-cell-tooth">
                  <div class="cc-perio-teeth-cell-img-container">
                    <img class="cc-perio-thumbToothImg-upper" src="{!URLFOR($Resource.Dental_Practice_Resources, tooth.t_image)}" />
                  </div>
                </div>
                <div class="cc-perio-teeth-cell-number-upper">
                  <div class="{!tooth.tooth_label_css}">{!tooth.tooth_num}</div>
                </div>                
              </div>
            </apex:repeat>
          </div>
          <div class="cc-perio-row-divider">
            <div>&nbsp;</div>
          </div>
          <div class="cc-perio-teeth-row-container-lower">
            <apex:repeat var="tooth" value="{!lower_variable_tooth_chart}">
              <div class="cc-perio-teeth-cell-container {!if(tooth.tooth_selected,'cc-perio-selected','')}" onclick="toggle_var_tooth('{!tooth.tooth_num}');" >
                <div class="cc-perio-teeth-cell-tooth">
                  <div class="cc-perio-teeth-cell-img-container">
                    <img class="cc-perio-thumbToothImg-upper" src="{!URLFOR($Resource.Dental_Practice_Resources, tooth.t_image)}" />
                  </div>
                </div>
                <div class="cc-perio-teeth-cell-number-upper">
                  <div class="{!tooth.tooth_label_css}">{!tooth.tooth_num}</div>
                </div>                
              </div>
            </apex:repeat>
          </div>
        </div>
        <!--end POT Search Holder-->
      </div> <!--End POT Selection Holder-->
      
    </div>
  </apex:outputpanel>  
  </apex:outputpanel>    
  <!---END Variable Tooth Selection Modal------->
  
  <!---START Removal Confirmation Modal------->
  <apex:outputpanel layout="block" rendered="{!confirmation_modal_display}">
  <div class="cc-patient-patient-search-wrapper" style="z-index: 1100; opacity: 1;">
    <div class="cc-patient-patient-search-container" style="box-shadow: rgba(0, 0, 0, 0.74902) 0px 0px 70px -15px;height:200px;width:50%;">
      <div class="cc-patient-patient-search-close">
        <div id="cc-patient-patient-search-close-button" onclick="cconfirmation_modal();">
          <i class="ccf-cancel-thin cc-color-font-black cc-cursor-hover"></i>
        </div>
      </div>
      <div class="cc-patient-patient-search-info" style="width: 100%; text-align: center; padding-top: 10px;">
        <apex:outputtext value="{!modal_out}" escape="false" /><br />

        <div class="cc-form-confirmTrash-buttonContainer">
          <div class="cc-form-confirmTrash-button cc-form-confirmTrash-button-cancel" onclick="cconfirmation_modal();">Cancel</div>
          <apex:dynamicComponent componentValue="{!confirmation_modal}"/>                   
        </div>
              
      </div>
    </div>
  </div>
  </apex:outputpanel>  
  <!--End Confirmation Modal------------------>
  
  
  </div> <!--end cc-content-container-->
</apex:outputpanel> <!--end cc-content-wrapper-->
</apex:outputpanel>

<script>  
  function refresh_top_nav() { 
    nav_bar_refresh();  
    setTimeout("refresh_top_nav()",600000); 
  }  

  window.onload = function() { 
    setTimeout("refresh_top_nav();",600000);  
    try {focus_to_upload();} catch (err) {}
  }
</script>

</apex:form>
</body>
</apex:page>