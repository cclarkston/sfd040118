              <div class="cc-center cc-tooltip">Display Notes</div>
            </apex:outputpanel>
            <apex:outputpanel layout="block" styleclass="cc-section-header-add cc-hover-tooltips-dark" onclick="change_legacy_display();" rendered="{!display_legacy_notes}">
