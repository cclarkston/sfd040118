                <div class="cc-perio-teeth-cell-number-upper">
                  <div class="{!tooth.tooth_label_css}">{!tooth.tooth_num}</div>
                </div>                
              </div>
            </apex:repeat>
          </div>
        </div>
        <!--end POT Search Holder-->
      </div> <!--End POT Selection Holder-->
      
    </div>
