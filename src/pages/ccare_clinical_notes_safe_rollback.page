                </apex:outputpanel>                
              </apex:outputpanel>
              <apex:outputpanel layout="block" styleclass="cc-patient-notes-addNote-past" style="display:flex;" rendered="{!if(fn.note.id!=current_note_edit_id,false,true)}"  >
                <apex:inputfield value="{!follow_up_note_dml.Doctor_Note__c}" style="width:100%;" />
                <apex:outputpanel layout="block" styleclass="cc-patient-schedule-row-date cc-clinical-appointment-date-completed cc-hover-tooltips-light" style="padding-left:30px;">
                  <apex:commandLink action="{!save_follow_up_form}" reRender="follow_notes_holder,error_holder" value="Save" styleclass="lblue" status="ajaxstatus"/>
                </apex:outputpanel>
                <apex:outputpanel layout="block" styleclass="cc-patient-schedule-row-date cc-clinical-appointment-date-scheduled cc-hover-tooltips-light ">
