                <apex:outputpanel layout="block" styleclass="cc-clinical-appointment-rows-date">
                  <apex:commandLink action="{!edit_follow_up_form}" reRender="follow_notes_holder,error_holder" value="Reset" styleclass="lblue" status="ajaxstatus">
                    <apex:param name="fnid" value="{!fn.note.id}" />
                  </apex:commandLink>  
                </apex:outputpanel>                
              </apex:outputpanel>              
            </div>
