                    <apex:param name="fnid" value="{!fn.note.id}" />
                  </apex:commandLink>                  
                </apex:outputpanel>
                <apex:outputpanel layout="block" rendered="{!fn.allow_edit}" style="padding-left:30px;" styleclass="cc-patient-schedule-row-date cc-clinical-appointment-date-completed cc-hover-tooltips-light">
                  <apex:commandLink action="{!confirm_delete_follow_up}" reRender="main_holder" value="Delete" styleclass="lblue" status="ajaxstatus">
                    <apex:param name="fnid" value="{!fn.note.id}" />
