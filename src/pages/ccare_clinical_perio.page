  <apex:param name="pcid" value="" />
</apex:actionFunction>
<apex:actionFunction name="delete_prosth_form" action="{!delete_prosth_exam_form}" rerender="main_holder" status="ajaxstatus" oncomplete="start_perio_form();"  >
  <apex:param name="pcid" value="" />
</apex:actionFunction>
