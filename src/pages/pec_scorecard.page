          <td><apex:outputfield value="{!tlist.subject}" /></td>
          <td><apex:outputfield value="{!tlist.Outcome__c}" /></td>
          <td><span style="color:blue;font-weight:bold;" onclick="use_fancy_cursor(this);score_task_record('{!tlist.id}')">Score</span></td>
